{
    "name": "react-responsive-list",
    "version": "0.0.1",
    "description": "ðŸ“‹ React Responsive List (Table)",
    "main": "dist/ResponsiveTable.js",
    "author":
        "Aleksandr Anokhin <sanohin@zoho.com> (http://github.com/sanohin)",
    "homepage": "https://github.com/sanohin/react-responsive-list",
    "repository": {
        "type": "git",
        "url": "https://github.com/sanohin/react-responsive-list.git"
    },
    "bugs": {
        "url": "https://github.com/sanohin/react-responsive-list.git"
    },
    "license": "MIT",
    "engines": {
        "node": ">= 0.10.0"
    },
    "scripts": {
        "build": "babel -d dist src --copy-files",
        "clean": "rm -rf dist && mkdir dist",
        "watch": "babel -w -d dist src --copy-files",
        "build-example":
            "browserify -t browserify-css ./example/example.js -o ./example/bundle.js -t [ babelify --presets [ es2015 react ] ]",
        "watch-example":
            "watchify -t browserify-css ./example/example.js -o ./example/bundle.js -t [ babelify --presets [ es2015 react ] ] -v",
        "serve-example": "http-server ./example/ -p 8080",
        "format":
            "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
        "start": "npm run build; npm run build-example; npm run serve-example",
        "deploy-example": "npm run build-example; gh-pages -d ./example",
        "test": "no tests yet!",
        "preversion": "npm run clean && npm run build",
        "postpublish": "git push && git push --tag"
    },
    "devDependencies": {
        "babel-cli": "^6.24.1",
        "babel-core": "^6.25.0",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babelify": "^8.0.0",
        "browserify": "^14.5.0",
        "browserify-css": "^0.14.0",
        "core-js": "^2.4.1",
        "gh-pages": "^1.0.0",
        "http-server": "^0.10.0",
        "prettier": "1.9.0",
        "prop-types": "^15.5.10",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-live": "1.7.1",
        "styled-components": "2.2.4",
        "watchify": "^3.9.0"
    },
    "browserify-css": {
        "autoInject": true,
        "minify": true,
        "rootDir": "."
    },
    "keywords": ["react", "react-component", "table", "list", "responsive"],
    "directories": {
        "test": "test"
    }
}
